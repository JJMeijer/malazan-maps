{
  "name": "malazan-maps",
  "version": "1.0.4",
  "author": "Johan <johan@malazanmaps.com>",
  "license": "MIT",
  "scripts": {
    "serve": "eleventy --serve",
    "build:prod": "cross-env NODE_ENV=production yarn build:all",
    "build:all": "yarn build:css && yarn build:js && eleventy",
    "watch:css": "tailwindcss -i src/css/main.css -o views/static/css/main-dist.css -w",
    "build:css": "tailwindcss -i src/css/main.css -o views/static/css/main-dist.css --minify",
    "build:js": "node esbuild.js prod",
    "watch:js": "node esbuild.js watch",
    "wiki": "node tools/wiki.js",
    "validate": "node tools/validate.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "axios": "^0.22.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.24.0",
    "esbuild": "^0.13.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "fuse.js": "^6.4.6",
    "html-minifier": "^4.0.0",
    "node-html-parser": "^4.1.5",
    "prettier": "^2.4.1",
    "prompts": "^2.4.1",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  },
  "dependencies": {}
}
