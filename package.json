  "scripts": {
    "serve": "eleventy --serve",
    "build": "yarn build:css && yarn build:js && eleventy",
    "build:css": "postcss src/css/main.css -o views/static/css/main-dist.css",
    "watch:css": "postcss -w src/css/main.css -o views/static/css/main-dist.css",
    "build:js": "node esbuild.js prod",
    "watch:js": "node esbuild.js watch",
    "wiki": "node tools/wiki.js",
    "slugify": "node tools/slugify.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@sindresorhus/slugify": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "axios": "^0.22.0",
    "colors": "^1.4.0",
    "date-fns": "^2.24.0",
    "esbuild": "^0.13.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "fuse.js": "^6.4.6",
    "html-minifier": "^4.0.0",
    "node-html-parser": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-cli": "^9.0.1",
    "postcss-import": "^14.0.2",
    "postcss-minify": "^1.1.0",
    "prettier": "^2.4.1",
    "prompts": "^2.4.1",
    "stylelint": "^13.13.1",
