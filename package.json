{
    "name": "malazan-maps",
    "version": "1.1.10",
    "author": "Johan <johan@malazanmaps.com>",
    "license": "MIT",
    "scripts": {
        "serve": "eleventy --serve",
        "build:prod": "cross-env NODE_ENV=production yarn build:all",
        "build:all": "yarn css:build && yarn js:build && eleventy",
        "css:watch": "tailwindcss -i src/css/main.css -o views/static/css/main-dist.css -w",
        "css:build": "tailwindcss -i src/css/main.css -o views/static/css/main-dist.css --minify",
        "js:build": "node esbuild.js prod",
        "js:watch": "node esbuild.js watch",
        "add-data": "node tools/add-data",
        "validate": "node tools/validate",
        "stats": "node tools/stats",
        "lint": "eslint -c .eslintrc"
    },
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "ajv": "^8.11.0",
        "ajv-formats": "^2.1.1",
        "axios": "^0.27.2",
        "colors": "^1.4.0",
        "cross-env": "^7.0.3",
        "date-fns": "^2.28.0",
        "esbuild": "^0.14.38",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fuse.js": "^6.5.3",
        "html-minifier": "^4.0.0",
        "node-html-parser": "^5.3.3",
        "prettier": "^2.6.2",
        "prompts": "^2.4.2",
        "slugify": "^1.6.5",
        "tailwindcss": "^3.0.24",
        "typescript": "^4.6.4"
    },
    "dependencies": {}
}
